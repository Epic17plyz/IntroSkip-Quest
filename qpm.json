{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Intro Skip",
    "id": "IntroSkip",
    "version": "1.2.1",
    "url": null,
    "additionalData": {
      "overrideSoName": "libIntroSkip.so"
    }
  },
"workspace": {
    "scripts": {
      "backup": [
        "pwsh ./scripts/get-backup.ps1 $0:?"
      ],
      "build": [
        "pwsh ./scripts/build.ps1 $0:?"
      ],
      "clean": [
        "qpm s build -- -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "log": [
        "pwsh ./scripts/start-logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh ./scripts/createqmod.ps1 $0:?"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1 $0:?"
      ],
      "restore": [
        "pwsh ./scripts/restore-backup.ps1 $0:?"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1 $0:?"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1 $0:?"
      ],
      "validate": [
        "pwsh ./scripts/validate-modjson.ps1 $0:?"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "IntroSkip"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "4007.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.54",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.22",
      "additionalData": {}
    }
  ]
}